{% set implemented = [
    "cta_simple_centered",
    "cta_simple_justified",
    "cta_split_with_image",
    "feature_two_by_two_grid",
    "feature_grid_icons_with_large_image",
    "feature_3_columns_with_icons",
    "hero_split_with_image",
    "hero_image_tiles",
    "hero_image_background",
    "featured_with_image",
    "stats_with_description",
    "stats_with_image",
    "stats_simple_grid",
    "faq_with_text",
    "product_overview_with_multiple_style_changes",
    "filter_product_view_grid",
] %}

{% for block in blocks %}
	{% if block.type.handle in implemented %}
		{% include "_components/" ~ block.type with { "block": block } %}
	{% endif %}
{% endfor %}
