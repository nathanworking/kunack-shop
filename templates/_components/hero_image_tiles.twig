<section class="dynamic_bg ani_default_container">
     <div class="relative isolate">
          <svg class="ani_default_element absolute inset-x-0 top-0 -z-10 h-[64rem] w-full stroke-gray-200 [mask-image:radial-gradient(32rem_32rem_at_center,white,transparent)]" aria-hidden="true">
               <defs>
               <pattern id="1f932ae7-37de-4c0a-a8b0-a6e3b4d44b84" width="200" height="200" x="50%" y="-1" patternUnits="userSpaceOnUse">
                    <path d="M.5 200V.5H200" fill="none" />
               </pattern>
               </defs>
               <svg x="50%" y="-1" class="overflow-visible fill-gray-50">
               <path d="M-200 0h201v201h-201Z M600 0h201v201h-201Z M-400 600h201v201h-201Z M200 800h201v201h-201Z" stroke-width="0" />
               </svg>
               <rect width="100%" height="100%" stroke-width="0" fill="url(#1f932ae7-37de-4c0a-a8b0-a6e3b4d44b84)" />
          </svg>
          <div class="ani_default_element absolute left-1/2 right-0 top-0 -z-10 -ml-24 transform-gpu overflow-hidden blur-3xl lg:ml-24 xl:ml-48" aria-hidden="true">
               <div class="aspect-[801/1036] w-[50.0625rem] bg-gradient-to-tr from-accent to-secondary-light opacity-30" style="clip-path: polygon(63.1% 29.5%, 100% 17.1%, 76.6% 3%, 48.4% 0%, 44.6% 4.7%, 54.5% 25.3%, 59.8% 49%, 55.2% 57.8%, 44.4% 57.2%, 27.8% 47.9%, 35.1% 81.5%, 0% 97.7%, 39.2% 100%, 35.2% 81.4%, 97.2% 52.8%, 63.1% 29.5%)"></div>
          </div>
          <div class="overflow-hidden">
               <div class="mx-auto max-w-7xl px-6 pb-32 pt-36 sm:pt-60 lg:px-8 lg:pt-32">
               <div class="mx-auto max-w-2xl gap-x-14 lg:mx-0 lg:flex lg:max-w-none lg:items-center">
                    <div class="w-full max-w-xl lg:shrink-0 xl:max-w-2xl">
                         <h1 class="ani_default_element type_4xl text-primary-dark">
                              {{ block.content_title_plain }}
                         </h1>
                         <p class="ani_default_element relative mt-6 type_lg text-secondary-dark sm:max-w-md lg:max-w-none">
                              {{ block.content_body_plain }}
                         </p>
                         <div class="ani_default_element mt-10 flex type_xs items-center gap-x-6">
                              {% include "_components/_shortcuts/neo_button_double.twig" %}
                         </div>
                    </div>
                    <div class="mt-14 flex justify-end gap-8 sm:-mt-44 sm:justify-start sm:pl-20 lg:mt-0 lg:pl-0">
                         <div class="ml-auto w-44 flex-none space-y-8 pt-32 sm:ml-0 sm:pt-80 lg:order-last lg:pt-36 xl:order-none xl:pt-80">
                              <div class="relative">
                              {% if block.image_max_five|length %}
                                   {% for image in block.image_max_five.limit(1).all() %}
                                        {% set photoDisplay = image.hero_optimization %}
                                             {% include "_partials/image_lazy" with {
                                                  class: 'image_tile_portrait ani_default_element'
                                             } %}
                                   {% endfor %}
                              {% endif %}
                              <div class="box_outline"></div>
                              </div>
                         </div>
                         <div class="mr-auto w-44 flex-none space-y-8 sm:mr-0 sm:pt-52 lg:pt-36">
                              <div class="relative ani_default_element">
                                   {% if block.image_max_five|length %}
                                        {% for image in block.image_max_five.offset(1).limit(1).all() %}
                                             {% set photoDisplay = image.hero_optimization %}
                                                  {% include "_partials/image_lazy" with {
                                                       class: 'image_tile_portrait'
                                                  } %}
                                        {% endfor %}
                                   {% endif %}
                              <div class="box_outline"></div>
                              </div>
                              <div class="relative ani_default_element">
                                   {% if block.image_max_five|length %}
                                        {% for image in block.image_max_five.offset(2).limit(1).all() %}
                                             {% set photoDisplay = image.hero_optimization %}
                                                  {% include "_partials/image_lazy" with {
                                                       class: 'image_tile_portrait'
                                                  } %}
                                        {% endfor %}
                                   {% endif %}
                                   <div class="box_outline"></div>
                              </div>
                         </div>
                         <div class="w-44 flex-none space-y-8 pt-32 sm:pt-0">
                              <div class="relative ani_default_element">
                                   {% if block.image_max_five|length %}
                                        {% for image in block.image_max_five.offset(3).limit(1).all() %}
                                             {% set photoDisplay = image.hero_optimization %}
                                                  {% include "_partials/image_lazy" with {
                                                       class: 'image_tile_portrait'
                                                  } %}
                                        {% endfor %}
                                   {% endif %}
                                   <div class="box_outline"></div>
                              </div>
                              <div class="relative ani_default_element">
                                   {% if block.image_max_five|length %}
                                        {% for image in block.image_max_five.offset(4).limit(1).all() %}
                                             {% set photoDisplay = image.hero_optimization %}
                                                  {% include "_partials/image_lazy" with {
                                                       class: 'image_tile_portrait'
                                                  } %}
                                        {% endfor %}
                                   {% endif %}
                                   <div class="box_outline"></div>
                              </div>
                         </div>
                    </div>
               </div>
               </div>
          </div>
     </div>
</section>